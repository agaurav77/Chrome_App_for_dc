{
  "manifest_version": 2,
  "name": "Screen Capture-aditya",
  "description": "grabs frames from screen capture api",
  "version": "1.0",
  "permissions": [
    "<all_urls>",
  "desktopCapture",
    "experimental"
  ],
  "sockets": {
    "tcp" : {
      "connect": ["192.168.110.222:411"]
    }
  },

  "app":{
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": false
  }
  }



}


